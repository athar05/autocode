{
  "name": "autocode",
  "displayName": "Autocode",
  "publisher": "AtharShakeel",
  "description": "An extension that provides user with ready to use code snippets, thereby reducing development time",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/athar05/autocode"
  },
  "icon": "AutoCode.png",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      }
    ]
  }
}
